{
  "name": "llm-code-assistant",
  "displayName": "LLM Code Assistant",
  "description": "Multi-LLM code assistant with brainstorming capabilities",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onCommand:llm.startChat",
    "onCommand:llm.brainstorm"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "llm.securityReview",
        "title": "Perform Security Review"
      },
      {
        "command": "llm.generateTests",
        "title": "Generate Test Cases"
      },
      {
        "command": "llm.visualizeArch",
        "title": "Visualize Architecture"
      },
      {
        "command": "llm.startPairProgramming",
        "title": "Start AI Pair Programming"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "llm.securityReview",
          "group": "llm@1"
        },
        {
          "command": "llm.generateTests",
          "group": "llm@1"
        }
      ]
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "@babel/generator": "^7.24.6",
    "@babel/parser": "^7.24.6",
    "axios": "^1.6.2",
    "diff-match-patch": "^1.0.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "@types/vscode": "^1.100.0",
    "typescript": "^5.8.3"
  }
}
